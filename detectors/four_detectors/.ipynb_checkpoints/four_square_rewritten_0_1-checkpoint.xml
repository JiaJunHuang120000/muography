      <segmentation type="CartesianGridXYStaggered" grid_size_x="4.88*cm" grid_size_y="4.88*cm" stagger_x="0" stagger_y="0" />
      <id>system:8,layer:8,slice:5,x:32:-16,y:-16</id>
