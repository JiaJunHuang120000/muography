      id="0"
      name="Pathch0"
      type="ZeroDegreeCalorimeterSiPMonTile"
      readout="MuographyHits"
      vis="InvisibleWithDaughters"
    >
      <position x="Detector_x_pos*sin(Detector_angle*0-45*deg)"         y="Detector_y_pos*cos(Detector_angle*0-45*deg)"         z="1*m+Detector_z_pos-1.22*cm+Detector_length*3/2+1.22*cm"/>
